{
    "name": "diepchiaser/fincode",
    "type": "library",
    "description": "Fincode gateway for Omnipay",
    "keywords": ["omnipay", "fincode"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Omnipay\\Fincode\\": "src/Fincode"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "diepdp",
            "email": "diepdp@kaiyouit.com"
        },
        {
            "name": "diepchiaser",
            "email": "diepchiaser@gmail.com"
        }
    ],
    "require": {
        "omnipay/common": "^3.0",
        "ramsey/uuid": "^4.7",
        "guzzlehttp/guzzle": "^7.0",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
